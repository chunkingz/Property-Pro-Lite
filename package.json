{
  "name": "property-pro-lite",
  "version": "1.0.0",
  "description": "![Property Pro Lite Logo](https://chunkingz.github.io/Property-Pro-Lite/img/assets/favicon/apple-icon.png \"Property Pro Lite Logo\")",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel server/ -d ./build/ -s",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./build/index.js",
    "dev": "nodemon server/index.js --exec babel-node",
    "lint": "eslint ./ --fix",
    "test": "nyc mocha ./server/tests/* --require @babel/register --require @babel/polyfill --exit",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chunkingz/Property-Pro-Lite.git"
  },
  "keywords": [
    "property",
    "property lease",
    "sell house",
    "buy house",
    "rent house",
    "house agents",
    "hotels",
    "houses in Naija"
  ],
  "author": "Fortune King",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chunkingz/Property-Pro-Lite/issues"
  },
  "homepage": "https://github.com/chunkingz/Property-Pro-Lite#readme",
  "dependencies": {
    "@babel/runtime": "^7.5.1",
    "bcrypt": "^3.0.6",
    "cloudinary": "^1.14.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.1.1",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/node": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.0",
    "@babel/register": "^7.4.4",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.4",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.0",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1"
  }
}
